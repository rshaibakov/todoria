    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-standard": "^29.0.0",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^3.2.3",
    "vite-plugin-checker": "^0.5.6",
    "vitest": "^0.28.5",
    "vue-tsc": "^1.0.9"
