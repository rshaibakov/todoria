{
  "name": "todoria",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "yarn@3.3.1",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "yarn lint:style && yarn lint:type && yarn lint:script",
